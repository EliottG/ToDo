{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "description": "ToDo list app",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "dama/doctrine-test-bundle": "^6.5",
        "doctrine/annotations": "^1.11",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.5",
        "sensio/framework-extra-bundle": "^5.6",
        "symfony/asset": "4.4.*",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-bundle": "4.4.*",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.3",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/security": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/security-core": "4.4.*",
        "symfony/security-csrf": "4.4.*",
        "symfony/security-guard": "4.4.*",
        "symfony/security-http": "4.4.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "4.4.*",
        "symfony/var-dumper": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*",
        "symfony/yaml": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
      "reset-db" : [
        "symfony console doctrine:database:drop -f --env=dev",
        "symfony console doctrine:database:create --env=dev",
        "symfony console doctrine:schema:update -f --env=dev",
        "symfony console doctrine:fixtures:load -n --env=dev"
      ],
      "reset-db-test" : [
        "symfony console --env=test  doctrine:database:drop -f",
        "symfony console  --env=test doctrine:database:create",
        "symfony console  --env=test doctrine:schema:update -f",
        "symfony console  --env=test  doctrine:fixtures:load -n"
      ],
        "auto-scripts": {
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/maker-bundle": "^1.25"
    }
}
